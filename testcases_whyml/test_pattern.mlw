use int.Int
let rec search i l =
	match l with
	| Nil -> i
	| Cons x r -> if x = 0 then i else search (i+1) r
	end

let rec search2 i l =
	match l with
	| Nil2 -> i
	| Cons2 x r -> if x = 0 then i else search2 (i+1) r
	end

let rec test_pat xs =
match xs with
| Nil -> 0
| Cons x (Cons _ Nil | Nil) -> x
| Cons x ((Cons y ys) as x2) -> x + (test_pat x2)
end

